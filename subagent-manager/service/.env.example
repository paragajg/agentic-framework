# Subagent Manager Configuration

# Service
SUBAGENT_HOST=0.0.0.0
SUBAGENT_PORT=8001
SUBAGENT_LOG_LEVEL=INFO

# LLM Provider Configuration
SUBAGENT_LLM_PROVIDER=mock  # Options: mock, anthropic, openai
SUBAGENT_LLM_MODEL=mock-model
SUBAGENT_LLM_TEMPERATURE=0.7
SUBAGENT_LLM_MAX_TOKENS=4096

# API Keys (required for production)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Lifecycle Management
SUBAGENT_DEFAULT_TIMEOUT=300
SUBAGENT_MAX_LIFETIME=3600
SUBAGENT_CLEANUP_INTERVAL=60
SUBAGENT_MAX_CONCURRENT_SUBAGENTS=100
SUBAGENT_MAX_CONTEXT_TOKENS=100000

# Schema Registry
SUBAGENT_SCHEMA_REGISTRY_PATH=docs/schema_registry

# Observability
SUBAGENT_ENABLE_METRICS=true
SUBAGENT_ENABLE_TRACING=true
